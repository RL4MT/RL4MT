<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="at.ac.tuwien.big.momot.feature"
      label="%featureName"
      version="2.0.0.qualifier"
      provider-name="%providerName"
      plugin="at.ac.tuwien.big.momot.branding">

   <description url="http://martin-fleck.github.io/momot/">
      MOMoT is a framework that combines model-driven engineering (MDE) techniques with search-based optimization (population-based search and local search) to solve highly complex problems on model level.
   </description>

   <url>
      <update label="MOMoT Update Site" url="http://martin-fleck.github.io/momot/eclipse/updates/"/>
      <discovery label="Henshin" url="http://download.eclipse.org/modeling/emft/henshin/updates/release/"/>
      <discovery label="Xtext" url="http://download.eclipse.org/modeling/tmf/xtext/updates/composite/releases/"/>
   </url>

   <requires>
      <import plugin="com.google.guava" version="15.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.apache.commons.lang" version="2.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.henshin.interpreter"/>
      <import plugin="org.eclipse.emf.henshin.statespace"/>
      <import plugin="org.eclipse.emf.henshin.model"/>
      <import plugin="org.eclipse.ocl.ecore"/>
      <import plugin="org.eclipse.core.runtime" version="3.9.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.8.101" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="at.ac.tuwien.big.moea"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="at.ac.tuwien.big.momot.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="at.ac.tuwien.big.momot.branding"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
